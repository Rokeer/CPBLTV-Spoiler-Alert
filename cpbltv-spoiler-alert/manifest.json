{
  "manifest_version": 2,
 
  "name": "CPBLTV Spoiler Alert",
  "version": "0.0.1",
  "description": "Hide game results on the CPBLTV website.",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://www.cpbltv.com/lists.php*"]
  }],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
  	"default_title": "Set this page's color.",
    "default_icon" : "temp.png",
    "default_popup" : "popup.html"
  },
  "icons": {
    "16": "temp.png",
    "48": "temp.png",
    "128": "temp.png"
  }
}